<!--  ****************************************   SET UP PAGE  ****************************************    -->
<!--MODALS here as they need to be outside any div with fixed position ****************************************  -->
<div class="modal fade" id="myModal" role="dialog">
    <div class="modal-dialog">

        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title">Stat Collector's Players</h4>
            </div>
            <div class="modal-body">
                <ul class ="list-group" id="modalContent"></ul>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-warning" id="modalClose" data-dismiss="modal">Back</button>
                <button type="button" class="btn btn-success" id="modalNext" data-dismiss="modal">Next</button>
            </div>
        </div>

    </div>
</div>

<!-- ******************************************** pitch modal ********************************************  -->
<div class="modal fade" id="modalPitch" role="dialog">    <!-- pop up for next steps on pitch page -->
    <div class="modal-dialog">

        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 id="modalPitchHeader" class="modal-title"></h4>
            </div>
            <div class="modal-body">
                <div  id="modalPitchContent"></div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-danger" id="modalPitchNext" data-dismiss="modal">Next</button>
            </div>
        </div>

    </div>
</div>

<!-- ***************************************************** SETUP HEADER ***************************************************************** -->
<div id="setup" class="container-fluid">
    <div class="col-md-4 col-sm-4 col-xs-1"> </div>
    <div>
        <div class="col-md-4 col-sm-6 col-xs-12">
            <div class="imgHolder"><img src="./images/whatstat_logo.png" alt="whatstat logo"> </div>
        </div>
    </div>
</div>      <!-- END SETUP  -->


<div class="" id="setupA">
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-4 col-sm-4 col-xs-1"> </div>  <!-- spacer row  -->
            <div class="col-md-4 col-sm-6 col-xs-10">
                <form id="sportSetup">
                    <div class="form-group">
                        <!--<label for="sportSelect">Select Sport:</label> -->
                        <select class="form-control" id="sportSelect" name="sportSelect"></select>
                    </div>
                    <div class="form-group">
                        <input class="form-control" id="homeTeamInput" name="homeTeamInput" placeholder="Home Team" type="text" />
                    </div>
                    <div class="form-group">
                        <input class="form-control" id="awayTeamInput" name="awayTeamInput" placeholder="Away Team" type="text" />
                    </div>
                    <div class="form-group">
                        <!--   <label for="minsPerHalf">Mins per Half</label> -->
                        <select class="form-control select" id="minsPerHalf"></select>
                    </div>
                    <div class="form-group">
                        <!--    <label for="numberPlayers"> No. Players per Team</label> -->
                        <select class="form-control select" id="numberPlayers" name="numberPlayers"></select>
                    </div>

                    <div class="form-group">
                        <!--    <label for="numStatCollectors"> No. Stat Collectors</label> -->
                        <select class="form-control select" id="numStatCollectors" name="numStatCollectors"> </select>
                    </div>
                    <div class="form-group">
                        <div class="btn btn-primary form-control" id="setupSubmitBtn" name="submit" type="submit">Submit</div>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>
<!-- end setupA -->

<div class="bootstrap-iso" id="setupB">

    <div class="container-fluid">
        <div class="row">
            <div class="col-md-4 col-sm-0 col-xs-1"> </div><!-- spacer row  -->
            <div class="col-md-4 col-sm-12 col-xs-10" id="">
                <form method="post" id="addStats">
                    <div class="form-group"><input class="form-control" id="addStatsInput" name="addStatsInput" placeholder="Add Extra Events Here" type="text" /></div>
                </form>
            </div>
        </div>

        <div class="row">
            <div class="col-md-4 col-sm-0 col-xs-1"> </div>
            <!-- spacer row  -->
            <div class="col-md-4 col-sm-12 col-xs-10" id="gameStats"></div>      <!-- stat buttos get gappended to this div -->
        </div>

        <div class="row">
            <div class="col-md-4 col-sm-0 col-xs-1"> </div><!-- spacer row  -->
            <div class="col-md-4 col-sm-12 col-xs-10">
                <div class="addTopGap">

                    <div class="btn btn-danger rr"  id="removeStats" name="removeStats" >Remove </div>
                    <div class="btn btn-warning rr"  id="refreshStats" name="removeStats">Refresh </div>
                </div>
                <div class="addTopGap">
                    <div class="btn btn-primary" id="submitStats" name="submitStats" >Submit</div>
                </div>
            </div>
        </div>
    </div>
</div>  <!-- END SETUPB -->

<div class="bootstrap-iso" id="setupC">
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-4 col-sm-0 col-xs-0"></div>
            <div class="col-md-4 col-sm-12 col-xs-12 " id="playerButtons"></div> <!-- player buttons get appended to this div -->
        </div>
        <!-- **************************************** -->
        <div class="row">
            <div class="col-md-4 col-sm-0 col-xs-0"></div>
            <div class="col-md-4 col-sm-12 col-xs-12" >
                <div id="playersPerCollector" class="addTopGap"></div>
                <div class="well well-lg nmWell" id="statCollectorWell">
                    <div class="row">SELECT PLAYERS FOR STAT COLLECTOR <span id="statCollectorSpan" class="badge pull-right"> </span>
                    </div>
                </div>
                <div class="addTopGap">
                    <button class="btn btn-primary" type="button"  id="collectorStats" name="collectorStats" data-toggle="modal" data-target="#myModal">Submit</button>  <!-- triggers the modal div -->
                    <!--<button class="btn btn-warning" type="button"  id="arrangeButtons" name="arrangeButtons" >Arrange</button> -->
                </div>
            </div>
        </div>

    </div>
</div>
<!-- END SETUPC -->




<!--   **************************************************   EMAIL INVITE USERS PAGE   ***********************************************     -->


<div id="inviteUsers" class="container-fluid">
    <div class="row">
        <div class="col-md-4 col-sm-6 col-xs-0"></div>
        <div class="col-md-4 col-sm-6 col-xs-12">

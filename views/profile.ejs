<!--   ***************************************************  GAME START  **************************************************************     -->

<div id="gameStart" class="container-fluid">
    <div class="row">
        <div class="col-md-2 col-sm-0 col-xs-0">

        </div>

        <div class="col-md-3 col-xs-4">
            <div class="imgHolder"  id="homeTeamLogo"><img class="img-circle" src="./images/dlspLogo.png" alt="DLSPlogo" ></img></div>
            <div class="teamsScore" id="homeTeamScore"><h3>0</h3></div>
            <div class="teamtext homeTeamText"></div>
            <div class="teamCardHolder" id="homeTeamCardHolder"></div>   <!-- for yellow cards etc -->
        </div>

        <div class="col-md-3 col-xs-4" >
            <div id="clockButton" >
                <div class="imgHolder"><img src="./images/whatstat_logo.png" alt="whatstat logo" ></img></div>

            <div class="btn btn-primary col-lg-12 col-sx-12" id="statsBackBtn">HOME</div>
           <!-- <div class="btn btn-primary col-lg-12 col-sx-12" id="test">make chart</div>-->
        </div>


        <div class="col-md-3 col-xs-4">
            <div class="imgHolder" id="awayTeamLogo"><img class="img-circle" src="./images/marysLogo.png" alt="Maryslogo" ></img></div>
            <div class="teamsScore" id="awayTeamScore"><h3>0</h3></div>
            <div class="teamtext awayTeamText" ></div>
            <div class="teamCardHolder" id="awayTeamCardHolder"></div>    <!-- for yellow cards etc -->

        </div>

    </div> <!-- end row -->

    <div class="col-md-2 col-sm-0 col-xs-0"></div>
    <div id="statEventsHolder" class="col-md-9 col-sm-12 col-xs-12">
        <div id="statEvents"></div>     <!-- stat descriptions buttons get appended here -->
        <div id="teamEvents"></div>      <!-- team events e.g. Scrum / Lineout from teamEvents[] get appended here -->
    </div>

    <div id="statNumbers" class="col-md-9 col-sm-12 col-xs-12">
        <div id="homeStatNumbers"></div>
        <div id="awayStatNumbers"></div>
    </div>    <!-- player numbers buttons get appended here -->

    <div id ="statSummaryDisplay">
        <div id="statsSumtextHolder"></div>
    </div> <!-- text description on bottom of screen after each event. Displays moost recent event  -->

</div>

